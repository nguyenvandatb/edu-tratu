<!DOCTYPE html>
<html>
  <head>
    <title><%= Rails.application.class.parent_name %></title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <div class="main" id="main">
      <%= render "layouts/header" %>
        <div class="container">
          <%= yield %>
        </div>
      <%= render "layouts/footer" %>
    </div>
  </body>
</html>
